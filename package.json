    "prom-client": "^12.0.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-customizable-progressbar": "^1.0.3",
    "react-dom": "17.0.2",
    "tailwind-container-break-out": "^2.0.1",
    "tailwindcss-children": "^2.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "autoprefixer": "^10.4.1",
    "axe-core": "^4.3.3",
