    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "lint-staged": "^11.2.0",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.0.1",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.11"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
